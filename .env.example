# Required: Comma-separated Gemini API keys from different Google Cloud projects
GEMINI_API_KEYS=AIzaSy...key1,AIzaSy...key2,AIzaSy...key3

# Optional: Server configuration
PORT=8000
HOST=0.0.0.0

# Optional: Rate limits (Free tier defaults)
DEFAULT_RPD_LIMIT=250
DEFAULT_RPM_LIMIT=10

# Optional: Retry configuration
MAX_RETRIES=3
RETRY_DELAY_SECONDS=2

# Optional: Gemini API base URL
GEMINI_BASE_URL=https://generativelanguage.googleapis.com

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
